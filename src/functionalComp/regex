// @nums:  Array  @target : number, @return : array  length: 2 [,]
var fn = function(nums, target) {
    for(var i = 0; i< nums.length; i++) {
        let indexArray = target - nums[i];
        for(var j = 0;j< nums.length; j++) {
            if(i !== j && indexArray === nums[j])
                 return [i,j];
        }
    }
}

